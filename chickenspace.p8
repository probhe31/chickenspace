pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
-- chicken space
-- by probe31
hp = 10

p1 = {}
p1.x = 0
p1.y = 64
p1.h = 8
p1.w = 8
p1.speed = 0.8
p1.cdwn = 0
p1.wait = false
p1.cursp = 1

bullets = {}
function create_bullet()
	b = {}
	b.x = p1.x + 6
	b.y = p1.y
	b.h = 8
	b.w = 8
	add(bullets, b)
end


function update_bullets()
	for b in all(bullets) do 
		b.x += 3
		if b.x > 120 then
			create_egg_explosion(b.x, b.y)
			del(bullets, b)
		end
		
		for e in all(enemies) do 
	 	if collide(b,e) then
	 	 	create_egg_explosion(b.x, b.y)
	 	 	del(enemies, e)
	 	 	del(bullets, b)	 	 
	 	end
		end
	end
end

function draw_bullets()
	for b in all(bullets) do 
	 spr(6,b.x, b.y)
	end
end


eggs_y = {}
function create_egg_y(x,y,sz,col,spd)
 e = {}
 e.x = x
 e.y = y+4
 e.sz = sz
 e.col = col
 e.spd = spd
 add(eggs_y, e)
end

function update_eggs_y()
	for e in all(eggs_y) do 
	 e.sz-= e.spd
	 if e.sz <= 1 then
	  del(eggs_y,e)
	 end
	end
end

function draw_eggs_y()
 for e in all(eggs_y) do 
	 circfill(e.x, e.y, e.sz, e.col)
	end
end


enemies = {}
function create_enemy(x,y,spd,sp)
	e = {}
	e.x = x
	e.y = y
	e.w = 8
	e.h = 8
	e.spd = spd
	e.sp = sp
	add(enemies, e)	
end

function update_enemies()
 for e in all(enemies) do 
  e.x -= e.spd
  if e.x < 20 then
  	del(enemies, e)
  end
	end
end

function draw_enemies()
 for e in all(enemies) do 
  spr(e.sp,e.x,e.y)
	end
end


current_hord = 1
--hord_size = 50
--t_hord1 = 50
c_hord = 0
ct_hord = 0


function gen_lvl(hord_size, ttg)
 ct_hord+=1
 if ct_hord > ttg then
  ct_hord = 0
  c_hord+=1 
  	if c_hord > hord_size then
    	current_hord += 1
	else
   		create_enemy(128,rnd(60)+40, 0.5, 3)
 	end
 end   
end

function create_egg_explosion(x, y)
	create_egg_y(x,y,rnd(8)+5,7,0.25)
	create_egg_y(x+rnd(2),y+rnd(2),rnd(5)+3,10,0.2)
end

function draw_hearts()
	rectfill(0,0,128,9,7)
 rectfill(1,1,126,8,15)
	for i=0, hp do  
		spr(4, 2+i*8, 0)
	end
end

function _init()
end

function _update60()
 
 	gen_lvl(50,50)
	--if(current_hord==1)then gen_lvl1() end
 	--if(current_hord==2)then gen_lvl2() end
	--if(current_hord==3)then gen_lvl3() end
 	--if(current_hord==4)then gen_lvl4() end
 	--if(current_hord==5)then gen_lvl5() end
 	--if(current_hord==6)then gen_lvl6() end
 	--if(current_hord==7)then gen_lvl7() end
 	--if(current_hord==8)then gen_lvl8() end
 	--if(current_hord==9)then gen_lvl9() end
 	--if(current_hord==10)then gen_lvl0() end
   
	if btn(0) then p1.x-=p1.speed end
	if btn(1) then p1.x+=p1.speed end
	if btn(2) then p1.y-=p1.speed end
	if btn(3) then p1.y+=p1.speed end
	
	if btn(4) then
	 
	 if not p1.wait then
	  p1.wait = true
	  create_bullet()
	 else
	 	p1.cdwn+=1
	 	if p1.cdwn > 5 then
	 	 p1.cdwn = 0
	 		p1.wait = false
	 	end 
	 end	 
	end

	update_bullets()
	update_eggs_y()
	update_enemies()
end

spdanim = 8
c_spdanim = 0
function _draw()	
	cls(2)
	map(0,0,0,0,16,16)
 c_spdanim+=1
 if c_spdanim>spdanim then 
  c_spdanim = 0 
  p1.cursp+=1
  if p1.cursp>2 then p1.cursp = 1 end
 end

 spr(p1.cursp,p1.x, p1.y)

	draw_bullets()
	draw_eggs_y()
	draw_enemies()
	draw_hearts()
end


function collide(a,b)
 res = false
 if a.x < b.x + b.w and 
    a.x + a.w > b.x and
    a.y < b.y + b.h and 
    a.y + a.h > b.y then
    res = true
 end  
 return res
end
